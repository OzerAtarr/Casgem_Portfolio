@model List<Casgem_Portfolio.Models.Entities.TblWhoAmI>

@foreach (var item in Model)
{
    <section class="w3l-index3" id="about">
        <div class="midd-w3 py-5">
            <div class="container py-lg-5 py-md-3">
                <div class="row">
                    <div class="col-lg-4">
                        <div class="position-relative">
                            <img src="~/Templates/alexander-starter/assets/images/photo1.jpeg" alt="" class="radius-image img-fluid">
                        </div>
                    </div>
                    <div class="col-lg-8 mt-lg-0 mt-5">
                        <h5 class="title-small mb-2">@item.Title1</h5>
                        <h3 class="title-big">@item.Title2</h3>
                        <p class="mt-4">
                            @item.Description
                        </p>
                        <a href=@item.CvUrl class="btn btn-style btn-primary mt-lg-5 mt-4" download>Cv İndir</a>
                    </div>
                </div>
            </div>
        </div>
    </section>
}